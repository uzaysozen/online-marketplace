#admin-form
  = simple_form_for(:banned_words, url: other_path) do |f|
    = f.error_notification

    #banned-words.form-inputs
      = f.input :content, label: false, as: :select, collection: [], input_html: { class: 'select2', multiple: true }
    .form-actions.text-center
      = f.button :button, class: 'btn-primary btn-md edit-btn' do
        Save changes