- @listings.each do |listing|
  %tr
    %td= listing.title
    %td= listing.description
    %td= listing.price
    %td= listing.creator_id
    %td= listing.receiver_id
    %td= listing.listing_category.name
    %td= listing.listing_condition.name
    %td= listing.listing_status.name
    %td
      = link_to 'Edit', edit_listing_path(listing), class: 'ajax-modal'
      %br
      = link_to 'Delete', listing, method: :delete, data: { confirm: 'Are you sure?' }